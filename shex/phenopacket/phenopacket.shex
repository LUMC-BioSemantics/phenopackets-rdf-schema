PREFIX : <http://purl.org/ejp-rd/phenopackets-rdf-schema/v200/shex/>
PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX sio: <http://semanticscience.org/resource/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:phenopacketShape IRI {
  a [sio:SIO_000015] ; # sio:information-content-entity
  a [obo:NCIT_C79269] ; # obo:participant-object
  sio:SIO_001403 @:individualShape? ; # sio:is-associated-with subject
  sio:SIO_001403 @:phenotypicFeatureShape* ; # sio:is-associated-with phenotypic_features
  sio:SIO_001403 @:biosampleShape* ; # sio:is-associated-with biosample
  sio:SIO_001403 @:interpretationShape* ; # sio:is-associated-with interpretations
  sio:SIO_001403 @:medicalActionShape* ; # sio:is-associated-with medical_action
  sio:SIO_001403 @:metadataShape . # sio:is-associated-with metadata
}

:individualShape IRI {
  a [sio:SIO_010375] ; # sio:multicellular-organism
  a [obo:NCIT_C41189] . # obo:study-subject
}

:phenotypicFeatureShape IRI {
  a [sio:SIO_000614] ; # sio:attribute
  a [obo:HP_~] . # URI for phenotype, e.g., HPO term URI
}

:biosampleShape IRI {
  a [sio:SIO_010412] ; # sio:specimen
  a [obo:NCIT_C43412] . # obo:biosample
}

:interpretationShape IRI {
  a [sio:SIO_001003] ; # sio:diagnostic-opinion
  a [obo:NCIT_C41255] . # obo:interpretation
}

:medicalActionShape IRI {
  a [sio:SIO_001398] ; # sio:medical-intervention
  a [obo:NCIT_C25218] . # obo:Intervention-or-procedure
}

:metadataShape IRI {
  a [sio:SIO_001330] ; # sio:metadata
  a [obo:NCIT_C52095] . # obo:metadata
}
